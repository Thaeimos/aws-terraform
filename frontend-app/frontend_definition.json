{
  "containerDefinitions": [ 
     { 
        "essential": true,
        "image": "ECR_URL_PLACEHOLDER:latest", 
        "logConfiguration": { 
           "logDriver": "awslogs",
           "options": { 
              "awslogs-group" : "NAME_PLACEHOLDER",
              "awslogs-region": "REGION_PLACEHOLDER",
              "awslogs-stream-prefix": "ecs"
           }
        },
        "name": "NAME_PLACEHOLDER",
        "portMappings": [ 
           { 
              "containerPort": 3000,
              "hostPort": 3000,
              "protocol": "tcp"
           }
        ],
        "environment": [
            {
               "name": "Environment",
               "value": "DEPRECATED"
            },
            {
               "name": "AWS_XRAY_DAEMON_ADDRESS",
               "value": "xray-sidecar:2000"
            },
            {
               "name": "AWS_XRAY_DEBUG_MODE",
               "value": "TRUE"
            }
        ],
        "secrets": [
            {
               "name": "Test_v",
               "valueFrom": "FRONT_SECRET_PLACEHOLDER:username::"
            }
         ],
         "links": [
            "xray-daemon"
          ]
     },
     {
      "environment": [],
      "name": "xray-sidecar",
      "mountPoints": [],
      "image": "amazon/aws-xray-daemon",
      "cpu": 32,
      "memoryReservation": 256,
      "portMappings": [
          {
              "protocol": "udp",
              "containerPort": 2000,
              "hostPort": 0
          }
      ],
      "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
              "awslogs-region": "REGION_PLACEHOLDER",
              "awslogs-stream-prefix": "ecs",
              "awslogs-group": "NAME_PLACEHOLDER"
          }
      },
      "essential": true,
      "volumesFrom": []
  }
  ],
  "cpu": "768",
  "family": "NAME_PLACEHOLDER",
  "memory": "768",
  "executionRoleArn": "FRONT_EXEC_ROLE_TASK_PLACEHOLDER",
  "requiresCompatibilities": [ 
      "EC2" 
   ]
}