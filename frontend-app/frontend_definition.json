{
  "containerDefinitions": [ 
     { 
        "essential": true,
        "image": "790577265452.dkr.ecr.eu-west-2.amazonaws.com/frontend-app-ecs-ec2:latest",
        "logConfiguration": { 
           "logDriver": "awslogs",
           "options": { 
              "awslogs-group" : "frontend-app-ecs-ec2",
              "awslogs-region": "eu-west-2",
              "awslogs-stream-prefix": "ecs"
           }
        },
        "name": "frontend-app-ecs-ec2",
        "portMappings": [ 
           { 
              "containerPort": 3000,
              "hostPort": 3000,
              "protocol": "tcp"
           }
        ],
        "environment": [
          {
              "name": "environment",
              "value": "TEST"
          }
        ]
     }
  ],
  "cpu": "256",
  "family": "frontend-app-ecs-ec2",
  "memory": "512",
  "requiresCompatibilities": [ 
      "EC2" 
   ]
}