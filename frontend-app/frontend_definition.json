{
  "containerDefinitions": [ 
     { 
        "essential": true,
        "image": "PLACEHOLDER_URL/PLACEHOLDER_NAME:latest",
        "logConfiguration": { 
           "logDriver": "awslogs",
           "options": { 
              "awslogs-group" : "PLACEHOLDER_NAME",
              "awslogs-region": "PLACEHOLDER_REGION",
              "awslogs-stream-prefix": "ecs"
           }
        },
        "name": "PLACEHOLDER_NAME",
        "portMappings": [ 
           { 
              "containerPort": 3000,
              "hostPort": 3000,
              "protocol": "tcp"
           }
        ],
        "environment": [
          {
              "name": "environment",
              "value": "PLACEHOLDER_ENV"
          }
        ]
     }
  ],
  "cpu": "256",
  "family": "PLACEHOLDER_NAME",
  "memory": "512",
  "requiresCompatibilities": [ 
      "EC2" 
   ]
}